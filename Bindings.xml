<Bindings>
  <!-- Toggle options -->
  <Binding name="DEJUNK_TOGGLE_OPTIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_ToggleOptions()
  </Binding>

  <!-- Start selling -->
  <Binding name="DEJUNK_START_SELLING" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_StartSelling()
  </Binding>

  <!-- Start destroying -->
  <Binding name="DEJUNK_START_DESTROYING" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_StartDestroying()
  </Binding>

  <!-- Add/rem sell inclusions -->
  <Binding name="DEJUNK_ADD_SELL_INCLUSIONS" header="DEJUNKBLANK1" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "inclusions")
  </Binding>
  <Binding name="DEJUNK_REM_SELL_INCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "inclusions")
  </Binding>

  <!-- Add/rem sell exclusions -->
  <Binding name="DEJUNK_ADD_SELL_EXCLUSIONS" header="DEJUNKBLANK2" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("sell", "exclusions")
  </Binding>
  <Binding name="DEJUNK_REM_SELL_EXCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("sell", "exclusions")
  </Binding>

  <!-- Add/rem destroy inclusions -->
  <Binding name="DEJUNK_ADD_DESTROY_INCLUSIONS" header="DEJUNKBLANK3" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "inclusions")
  </Binding>
  <Binding name="DEJUNK_REM_DESTROY_INCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "inclusions")
  </Binding>

  <!-- Add/rem destroy exclusions -->
  <Binding name="DEJUNK_ADD_DESTROY_EXCLUSIONS" header="DEJUNKBLANK4" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_AddToList("destroy", "exclusions")
  </Binding>
  <Binding name="DEJUNK_REM_DESTROY_EXCLUSIONS" category="BINDING_CATEGORY_DEJUNK">
    DejunkBindings_RemoveFromList("destroy", "exclusions")
  </Binding>
</Bindings>
